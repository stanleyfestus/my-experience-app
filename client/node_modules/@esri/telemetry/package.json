{
  "name": "@esri/telemetry",
  "version": "8.0.0",
  "description": "A JavaScript Implementation of the ArcGIS Telemetry Specification",
  "main": "dist/node/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/umd/telemetry.js",
  "contributors": [
    {
      "name": "Esri DC"
    }
  ],
  "scripts": {
    "prebuild": "npm run clean:dist",
    "build": "npm run build:esm && npm run build:node && npm run build:umd",
    "build:esm": "tsc --outDir ./dist/esm --declarationDir dist/types --declaration true",
    "build:node": "tsc --module commonjs --outDir ./dist/node",
    "build:umd": "INPUT_FILE=./dist/esm/index.js rollup -c ../../rollup.config.js",
    "clean:dist": "rimraf dist",
    "lint": "standard src",
    "test": "jest \".*\\.test\\.ts\" --notify",
    "test:watch": "jest \".*\\.test\\.ts\" --notify --watch",
    "package": "npm install && npm run build && npm publish --access public",
    "package-beta": "npm run build && npm publish --tag beta --access public"
  },
  "keywords": [
    "arcgis",
    "telemetry",
    "analytics",
    "tracking"
  ],
  "license": "Apache-2.0",
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "@types/node": "^17.0.21"
  },
  "gitHead": "41dacf618be47fb2039b8c6aa088347fddf5f3bf"
}
